## Example environment variables for running the backend
# Copy to a file named `.env` for local Docker compose or use your host env.

# Database
SPRING_DATASOURCE_URL=jdbc:postgresql://<HOST>:<PORT>/<DB>?sslmode=require
SPRING_DATASOURCE_USERNAME=<DB_USERNAME>
SPRING_DATASOURCE_PASSWORD=<DB_PASSWORD>

# JWT secret
APP_JWTSECRET=<BASE64_OR_SECRET>
APP_JWT_EXPIRATION_MS=1800000

# Spring profile and port (optional)
SPRING_PROFILES_ACTIVE=prod
SERVER_PORT=8080
